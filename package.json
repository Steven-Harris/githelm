{
  "name": "githelm",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.12.4",
  "scripts": {
    "dev": "vite",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "build": "vite build",
    "serve": "vite preview",
    "postinstall": "pnpm svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json && tsc -p tsconfig.node.json",
    "lint": "eslint --ext .js,.ts,.svelte .",
    "lint:fix": "eslint --ext .js,.ts,.svelte --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Steven-Harris/githelm.git"
  },
  "author": "steven-harris",
  "bugs": {
    "url": "https://github.com/Steven-Harris/githelm/issues"
  },
  "homepage": "https://githelm.com",
  "engines": {
    "node": ">=20.19.0 || >=22.12.0",
    "pnpm": ">=9.0.0"
  },
  "dependencies": {
    "@sentry/browser": "^9.33.0",
    "@sentry/svelte": "^9.33.0",
    "@sentry/sveltekit": "^9.33.0",
    "@sentry/vite-plugin": "^3.5.0",
    "firebase": "^11.9.1",
    "graphql": "^16.11.0",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@playwright/test": "^1.53.1",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.22.2",
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "@tailwindcss/vite": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.8",
    "@tsconfig/svelte": "5.0.4",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vite-pwa/assets-generator": "^1.0.0",
    "@vite-pwa/sveltekit": "^1.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-svelte": "^3.10.1",
    "firebase-tools": "^14.9.0",
    "jsdom": "^26.1.0",
    "msw": "^2.10.2",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "sharp": "0.34.2",
    "sharp-ico": "0.1.5",
    "svelte": "^5.34.9",
    "svelte-check": "^4.2.2",
    "svelte-eslint-parser": "^1.2.0",
    "svelte-preprocess": "^6.0.3",
    "tslib": "2.8.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0",
    "vite": "^7.0.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^1.0.1",
    "vitest": "^3.2.4",
    "workbox-window": "^7.3.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@firebase/util",
      "@sentry/cli",
      "@tailwindcss/oxide",
      "esbuild",
      "msw",
      "protobufjs",
      "re2",
      "sharp",
      "svelte-preprocess"
    ]
  }
}
