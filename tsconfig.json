{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "allowArbitraryExtensions": true,
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "moduleResolution": "bundler"
  }
}
